  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development node server/server ",
    "start": "npm run build && cross-env NODE_ENV=production node server/server",
    "dev:client": "cross-env NODE_ENV=development webpack --watch --config build/webpack.client.config.js",
    "dev:server": "cross-env NODE_ENV=development webpack --watch --config build/webpack.server.config.js",
    "build": "rimraf dist && npm run build:client && npm run build:server",
    "build:client": "cross-env NODE_ENV=production webpack --config build/webpack.client.config.js --progress",
    "build:server": "cross-env NODE_ENV=production webpack --config build/webpack.server.config.js --progress"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "cross-env": "^5.0.5",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "lru-cache": "^4.1.1",
    "vue": "^2.5.9",
    "vue-router": "^3.0.3",
    "vuex": "^3.1.0",
    "vuex-router-sync": "^5.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "devDependencies": {
    "autoprefixer": "^9.5.1",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-3": "^6.24.1",
    "chokidar": "^1.7.0",
    "clean-webpack-plugin": "^0.1.17",
    "copy-webpack-plugin": "^4.2.3",
    "css-loader": "^0.28.7",
    "file-loader": "^1.1.4",
    "memory-fs": "^0.4.1",
    "postcss-loader": "^3.0.0",
    "pug": "^3.0.1",
    "pug-html-loader": "^1.1.5",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "vue-loader": "^15.0.0",
    "vue-server-renderer": "^2.5.9",
    "vue-template-compiler": "^2.5.9",
    "webpack": "^3.8.1",
    "webpack-dev-middleware": "^1.12.2",
    "webpack-hot-middleware": "^2.21.0",
    "webpack-merge": "^4.1.1"
  }
